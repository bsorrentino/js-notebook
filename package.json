{
  "name": "@bsorrentino/jsnotebook",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bsorrentino/js-notebook.git"
  },
  "author": "bsorrentino <bartolomeo.sorrentino@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "commit": "cz",
    "clean": "lerna run cleanall && lerna clean -y",
    "cleanall": "npm run clean && zx zx-clean.mjs",
    "build": "lerna bootstrap --hoist --strict",
    "start": "lerna run start --parallel",
    "lerna:bump": "npx lerna version --conventional-commits --no-push",
    "lerna:github:publish": "npx lerna publish from-package --registry=https://npm.pkg.github.com",
    "lerna:addBuild": "lerna add @bsorrentino/jsnotebook-client --scope=javascript-notebook"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.3.2",
    "@types/jest": "^27.4.1",
    "@types/pouchdb": "^6.4.0",
    "buffer": "^6.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "http-server": "^14.1.0",
    "jest": "^27.5.1",
    "lerna": "^4.0.0",
    "parcel": "^2.3.2",
    "sass": "^1.49.8",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.5.5",
    "zx": "^5.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "bump": true,
      "commit": true,
      "changelog": false,
      "tag": true
    }
  }
}
