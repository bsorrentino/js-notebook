{
  "name": "@bsorrentino/jsnotebook",
  "version": "0.5.2",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bsorrentino/js-notebook.git"
  },
  "author": "bsorrentino <bartolomeo.sorrentino@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "commit": "cz",
    "cleanall": "lerna clean -y && zx zx-clean.mjs --all",
    "clean": "lerna clean -y && zx zx-clean.mjs",
    "build": "lerna bootstrap --hoist --strict",
    "start": "lerna run start --parallel",
    "lerna:bump": "npx lerna version --conventional-commits --no-push",
    "lerna:github:publish": "lerna publish --conventional-commits --registry=https://npm.pkg.github.com --force-publish",
    "lerna:addBuild": "lerna add @bsorrentino/jsnotebook-client --scope=javascript-notebook"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.3.2",
    "@types/pouchdb": "^6.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "http-server": "^14.1.0",
    "lerna": "^4.0.0",
    "parcel": "^2.3.2",
    "sass": "^1.49.8",
    "typescript": "^4.5.5",
    "zx": "^5.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "bump": true,
      "commit": true,
      "changelog": false,
      "tag": true
    }
  }
}
